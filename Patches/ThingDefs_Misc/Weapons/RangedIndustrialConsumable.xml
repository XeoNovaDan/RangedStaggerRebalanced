<?xml version="1.0" encoding="utf-8" ?>
<Patch>

    <!-- ========== Triple Rocket Launcher ========== -->
    <Operation Class="PatchOperationAdd">
        <xpath>*/ThingDef[defName="Bullet_Rocket"]/projectile</xpath>
        <value>
            <armorPenetrationBase>0.75</armorPenetrationBase>
            <stoppingPower>5.0</stoppingPower>
        </value>
    </Operation>

    <!-- ========== Doomsday Rocket Launcher ========== -->
    <Operation Class="PatchOperationAdd">
        <xpath>*/ThingDef[defName="Bullet_DoomsdayRocket"]/projectile</xpath>
        <value>
            <armorPenetrationBase>1.125</armorPenetrationBase>
            <stoppingPower>7.5</stoppingPower>
        </value>
    </Operation>

</Patch>